   <script>
	 $(function(){
		if( /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {
			var showToolbar = false
		}else{
			var showToolbar = [
		    "undo redo | bold italic | bullist numlist | link | removeformat"
		  ]
		}
		    tinymce.init({   
				selector: "#description",
				plugins : "paste autolink link lists charmap preview",
				toolbar: showToolbar,
				menubar: false,
				statusbar: false,
				content_css : "/app/css/style.css",
				default_link_target: "_blank",
				target_list: false,
				link_title: false,
				paste_as_text: true,
				paste_text_sticky : true,
				paste_text_sticky_default: true,
				link_assume_external_targets: true,
				extended_valid_elements: 'a[href|target=_blank]',
				height: "300",
		    });

  

		var checkLength = function(field){
			var max = 160;
			var len = $(field).val().length;

			v = $(field).val().substring(0,160)
			$(this).val(v);

			
			if(len > max){
				$(this).val($(field).val().substring(0, len-1));
			}
			if (len >= max) {
				$('#short-description-count').text(' you have reached the limit');
			} else {
			var char = max - len;
				$('#short-description-count').text(char + ' characters left');
			}
		}
		checkLength('#short-description');
   

       $('#short-description').keyup(function () {
       	
			var max = 160;
			var len = $(this).val().length;

			v = $(this).val().substring(0,160)
			 $(this).val(v);


			if(len > max){
				$(this).val($(this).val().substring(0, len-1));
			}
			if (len >= max) {
				$('#short-description-count').text(' you have reached the limit');
			} else {
			var char = max - len;
				$('#short-description-count').text(char + ' characters left');
			}
		}).bind('keypress', function(e) {
		  if ((e.keyCode || e.which) == 13) {
		    return false;
		  }
		});


      });
